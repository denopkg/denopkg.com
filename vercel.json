{
  "name": "denopkg",
  "version": 2,
  "alias": ["denopkg.now.sh", "denopkg.com"],
  "builds": [
    {
      "src": "api/*/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/home"
    },
    {
      "src": "/.*",
      "dest": "/api/github"
    }
  ]
}
